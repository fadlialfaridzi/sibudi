    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "http-errors": "~1.6.3",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.0",
    "preline": "^3.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.10",
